{
  "name": "gatsby-source-dog",
  "version": "0.0.15",
  "description": "Gatsby source plugin for pulling data into Gatsby from Dog API (https://dog.ceo/dog-api/)",
  "author": "Alex Luong",
  "license": "MIT",
  "main": "gatsby-node.js",
  "scripts": {
    "build": "yarn clean && babel src --out-dir dist --ignore **/__tests__",
    "watch": "babel --watch src --out-dir dist --ignore **/__tests__",
    "format": "prettier --write src/**/*.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "yarn run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "gatsby-source-filesystem": "^1.5.39"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0-beta.59"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/node": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "prettier": "^1.13.7"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexluong/gatsby-source-dog"
  },
  "bugs": {
    "url": "https://github.com/alexluong/gatsby-source-dog/issues"
  }
}
